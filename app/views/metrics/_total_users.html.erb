<%= javascript_include_tag "//www.google.com/jsapi", "chartkick" %>
<%= line_chart User.where(created_at: DateTime.now - 13.months..DateTime.now).group_by_month(:created_at).count, colors: ["#598D38"], library: { hAxis: { baselineColor: "#778899", title: "Show Last 12 Months", format: "MMM y"}, vAxis: { baselineColor: "#778899", title: "Number Of Users"}, title: "Total Users" } %>
